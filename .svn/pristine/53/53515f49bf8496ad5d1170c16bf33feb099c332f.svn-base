	protected SgfensPedido fetchSingleResult(ResultSet rs) throws SQLException
	{
		if (rs.next()) {
			SgfensPedido dto = new SgfensPedido();
			populateDto( dto, rs);
			return dto;
		} else {
			return null;
		}
		
